{% extends 'base.html.twig' %}

{% block title %}{{ 'explore.mars'|trans }}{% endblock %}

{% block header %}{{ 'explore.mars'|trans }}{% endblock %}

{% block form %}
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.enterData) }}
        <input type="submit" value="{{ 'show.final'|trans }}" />
        {{ form_rest(form) }}
    {{ form_end(form) }}
{% endblock %}

{% block result %}
    <div class='result'>
        {% for result in results %}
            {{ result|trans }} <br>
        {% endfor %}
    </div>
{% endblock %}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}flickr photos v.{{ version }}{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/framework/css/main.css') }}" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            <div class='header'>
                <h1>
                   {{ 'flickr photos'|trans }}
                    {#{% trans %}flickr photos{% endtrans %}#}
                </h1>
            </div>
        <div class='wrapper'>
            <div id='table'>
                {% block table %}{% endblock %}
            </div>
        </div>
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>

<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/framework/css/main.css') }}" rel="stylesheet" />
        {% endblock %}
        {#<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />#}
    </head>
    <body>
        {% block body %}
            <div class='header'>
                <h1>
                    {% block header %}{% endblock %}
                </h1>
            </div>
        <div class='wrapper'>
            <div id='table'>
                {% block table %}{% endblock %}
            </div>
            {% block form %}{% endblock %}
            {% block result %}{% endblock %}
        </div>
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
